        <TextView
            android:id="@+id/sabado_comida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Comida" />

        <TextView
            android:id="@+id/sabado_cena"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cena" />

        <!-- Domingo -->
        <TextView
            android:id="@+id/domingo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Domingo" />

        <TextView
            android:id="@+id/domingo_desayuno"
            android:layout_width="match_parent"
