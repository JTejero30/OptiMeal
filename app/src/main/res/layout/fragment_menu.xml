<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MVVM.Menu.Menu">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <!-- Lunes -->
        <TextView
            android:id="@+id/lunes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Lunes" />

        <TextView
            android:id="@+id/lunes_desayuno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Desayuno" />

        <TextView
            android:id="@+id/lunes_comida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Comida" />

        <TextView
            android:id="@+id/lunes_cena"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cena" />

        <!-- Martes -->
        <TextView
            android:id="@+id/martes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Martes" />

        <TextView
            android:id="@+id/martes_desayuno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Desayuno" />

        <TextView
            android:id="@+id/martes_comida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Comida" />

        <TextView
            android:id="@+id/martes_cena"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cena" />

        <!-- Miércoles -->
        <TextView
            android:id="@+id/miercoles"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
