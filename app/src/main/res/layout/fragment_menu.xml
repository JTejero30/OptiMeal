        <TextView
            android:id="@+id/miercoles_desayuno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Desayuno" />

        <TextView
            android:id="@+id/miercoles_comida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Comida" />

        <TextView
            android:id="@+id/miercoles_cena"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cena" />

        <!-- Jueves -->
        <TextView
