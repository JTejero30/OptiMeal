            android:text="Comida" />

        <TextView
            android:id="@+id/jueves_cena"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cena" />

        <!-- Viernes -->
        <TextView
            android:id="@+id/viernes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Viernes" />

        <TextView
            android:id="@+id/viernes_desayuno"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Desayuno" />

        <TextView
            android:id="@+id/viernes_comida"
